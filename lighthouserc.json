{
  "ci": {
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.9 }],
        "first-contentful-paint": ["error", { "minScore": 0.6 }],
        "resource-summary:font:count": ["warn", { "maxNumericValue": 1 }]
      }
    },
    "collect": {
      "numberOfRuns": 2,
      "startServerCommand": "npm run serve"
    }
  }
}
